{
  "presets": [
    "es2015",
    "react"
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    ["transform-async-to-module-method", {
      "module": "bluebird",
      "method": "coroutine"
    }],
    "transform-object-assign",
    "transform-class-properties",
    "transform-es2015-modules-commonjs"
  ]
}
