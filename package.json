{
  "name": "media-atom-maker",
  "description": "media atom editor",
  "scripts": {
    "build-autotrack": "autotrack -o public/video-ui/build/autotrack.js -p cleanUrlTracker,eventTracker,impressionTracker,maxScrollTracker,mediaQueryTracker,outboundFormTracker,outboundLinkTracker,pageVisibilityTracker,socialWidgetTracker,urlChangeTracker",
    "build": "webpack -p ./public/video-ui/src/app.js -o ./public/video-ui/build/app.js --config ./build_config/webpack.prod.conf.js",
    "build-dev": "webpack -o ./public/video-ui/build/app.js --config ./build_config/webpack.dev.conf.js --watch",
    "client-dev": "node ./webpack-dev-server.js",
    "lint": "eslint public/video-ui/src/**/*.js",
    "test": "jest"
  },
  "jest": {
    "roots": [
      "public/video-ui/src/test"
    ],
    "preset": "ts-jest/presets/js-with-ts"
  },
