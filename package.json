{
  "name": "media-atom-maker",
  "description": "media atom editor",
  "scripts": {
    "build": "webpack -p ./public/video-ui/src/app.js ./public/video-ui/build/app.js --config ./build_config/webpack.prod.conf.js",
    "build-dev": "webpack ./public/video-ui/src/app.js ./public/video-ui/build/app.js --config ./build_config/webpack.dev.conf.js --watch",
    "build-bookmarklet": "bookmarklet scripts/bookmarklets/delete-atom.js scripts/bookmarklets/delete-atom.bookmarklet",
    "client-dev": "node ./webpack-dev-server.js",
    "lint-video-ui": "eslint public/video-ui/src/",
    "lint-cloudformation": "jsonlint cloudformation/*.json --quiet",
    "lint": "npm run lint-video-ui && npm run lint-cloudformation",
    "test": "npm run lint"
  },
  "devDependencies": {
    "argparse": "^1.0.9",
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "bookmarklet": "0.0.6",
    "chalk": "^1.1.3",
    "crypto": "0.0.3",
    "css-loader": "^0.25.0",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "jsonlint": "^1.6.2",
    "lodash": "^4.17.4",
    "node-sass": "^3.10.1",
    "properties-reader": "0.0.15",
    "react-hot-loader": "^3.0.0-beta.5",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "xhr2": "^0.1.4"
  },
  "dependencies": {
    "aws-sdk": "^2.10.0",
    "moment": "^2.17.1",
    "panda-session": "^0.1.6",
    "querystringify": "0.0.4",
    "raven-js": "^3.9.1",
    "react": "^15.3.2",
    "react-datepicker": "^0.39.0",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.1",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-form": "^6.4.1",
    "redux-thunk": "^2.1.0",
    "reqwest": "^2.0.5",
    "valid-url": "^1.0.9"
  }
}
