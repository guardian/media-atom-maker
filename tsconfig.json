{
  "compilerOptions": {
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es2017",
      "jsx": "react",
      "alwaysStrict": true,
      "allowJs": true,
      "sourceMap": true,
      "lib": [
          "es2017",
          "dom"
      ],
      "skipLibCheck": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
  },
  "include": [
      "public/video-ui/src/**/*",
  ]
}
