{
  "name": "pluto-message-ingestion",
  "version": "0.0.0",
  "type": "module",
  "repository": "git@github.com:guardian/media-atom-maker.git",
  "author": "The Guardian <digitalcms.dev@theguardian.com>",
  "description": "Ingesting messages sent from Pluto via a Kinesis stream",
  "packageName": "media-service:media-atom-maker:pluto-message-ingestion",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --no-error-on-unmatched-pattern -c ./eslint.config.mjs",
    "lint-fix": "yarn lint --fix",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "tscconfig": "tsc --showConfig",
    "test": "vitest",
    "test:ci": "vitest --run",
    "build": "esbuild src/index.ts --bundle --minify --outfile=target/index.js --external:@aws-sdk --external:aws-sdk --platform=node"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "3.901.0",
    "@aws-sdk/credential-providers": "3.901.0",
    "@guardian/libs": "26.0.0",
    "esbuild": "0.25.10",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@guardian/eslint-config": "12.0.0",
    "@guardian/tsconfig": "^1.0.0",
    "@types/aws-lambda": "8.10.152",
    "eslint": "9.37.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "prettier": "3.3.3",
    "typescript": "5.5.2",
    "vitest": "3.2.4"
  }
}
