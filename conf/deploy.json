{
  "stack": "media-atom-maker",
  "defaultStacks": ["media-service"],
  "packages": {
    "media-atom-maker": {
      "type": "autoscaling",
      "data": {
        "bucket": "atom-maker-dist",
        "publicReadAcl": false
      }
    }
  },
  "recipes": {
    "default": {
      "depends": ["deploy"]
    },
    "deploy": {
      "actionsBeforeApp": ["media-atom-maker.uploadArtifacts", "media-atom-maker.deploy"]
    }
  }
}
