{
  "stack": "media-atom-maker",
  "packages": {
    "media-atom-maker": {
      "type": "aws-s3",
      "data": {
        "bucket": "media-atom-dist",
        "cacheControl": "max-age=60"
      }
    }
  },
  "recipes": {
    "default": {
      "depends": ["deploy"]
    },
    "deploy": {
      "actionsBeforeApp": ["media-atom-maker.uploadStaticFiles"]
    }
  }
}
